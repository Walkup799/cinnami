{
  "expo": {
    "name": "Security Roll",
    "slug": "Security ROll",
    "version": "1.0.0",
     "scheme": "cinnamiapp",        
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/cinnami_logo.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/cinnami_logo.jpeg"
    }
  }
}
